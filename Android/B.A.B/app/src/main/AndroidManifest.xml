<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.bab">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>  //네트워크 환경 확인 권한
    <uses-permission android:name="android.permission.INTERNET" />   //인터넷 접속 권한 허용

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Mainactivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Calendar" />
        <activity android:name=".Loading" />
        <activity android:name=".Developer"></activity>
    </application>

</manifest>